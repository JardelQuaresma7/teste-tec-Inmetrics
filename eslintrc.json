{
    "extends": [
      "eslint:recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true,
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
  }